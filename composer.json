{
    "name": "juzaweb/email-marketing",
    "type": "juzaweb-module",
    "license": "MIT",
    "description": "Email Marketing module for JuzaWeb CMS",
    "extra": {
        "juzaweb": {
            "name": "Email Marketing",
            "domain": "email_marketing",
            "providers": [
                "Juzaweb\\Modules\\EmailMarketing\\Providers\\EmailMarketingServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\Modules\\EmailMarketing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\Modules\\EmailMarketing\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
         "juzaweb/core": "dev-master"
    }
}
