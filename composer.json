{
    "name": "juzaweb/email-marketing",
    "type": "juzaweb-plugin",
    "license": "MIT",
    "description": "Email Marketing plugin",
    "extra": {
        "juzaweb": {
            "name": "Email Marketing",
            "domain": "email_marketing",
            "providers": [
                "Juzaweb\\EmailMarketing\\Providers\\EmailMarketingServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\EmailMarketing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Juzaweb\\EmailMarketing\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "juzaweb/proxies": "*"
    },
    "require-dev": {
         "juzaweb/modules": "^1.0",
         "juzaweb/dev-tool": "^1.0"
    }
}
